{
  "name": "ai-models-tg-bot",
  "version": "1.0.0",
  "description": "Telegram bot aggregating multiple AI models",
  "main": "dist/index.js",
  "scripts": {
    "dev": "nodemon --exec ts-node src/index.ts",
    "dev:worker": "ts-node src/worker.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "start:worker": "node dist/worker.js",
    "start:prod": "NODE_ENV=production node dist/index.js",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:studio": "prisma studio",
    "db:migrate": "prisma migrate dev",
    "docker:build": "docker compose -f docker-compose.prod.yml build",
    "docker:up": "docker compose -f docker-compose.prod.yml up -d",
    "docker:down": "docker compose -f docker-compose.prod.yml down"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@anthropic-ai/sdk": "^0.71.2",
    "@bull-board/api": "^6.16.4",
    "@bull-board/express": "^6.16.4",
    "@prisma/client": "^5.22.0",
    "axios": "^1.13.2",
    "bcryptjs": "^3.0.3",
    "bull": "^4.16.5",
    "cors": "^2.8.6",
    "dotenv": "^17.2.3",
    "elevenlabs": "^1.59.0",
    "express": "^4.22.1",
    "express-rate-limit": "^8.2.1",
    "google-auth-library": "^10.5.0",
    "ioredis": "^5.9.2",
    "jsonwebtoken": "^9.0.3",
    "nodemailer": "^8.0.1",
    "openai": "^6.16.0",
    "prisma": "^5.22.0",
    "replicate": "^1.4.0",
    "telegraf": "^4.16.3",
    "winston": "^3.19.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^25.0.10",
    "@types/nodemailer": "^7.0.9",
    "nodemon": "^3.1.11",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  }
}
